<h1 class="page-header">Artists Matching <%= params[:artist_name] %></h1>
<ul>
<% @artists.each do |artist| %>
  <li><%= artist.name %> | <%= link_to "Show", artist_path(artist.id) %></li>
  <%= artist.popularity %>
  <img src=<%= artist.images[2]['url'] %> alt="Mountain View" style="width:<%= artist.images[2]['width'] %>px;height:<%= artist.images[2]['height'] %>px;">
  <% artist.genres.each do |genre| %>
    <%= genre %>
  <% end %>
<% end %>
</ul>
